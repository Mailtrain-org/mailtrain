    "react-i18next": "^9.0.1",
    "react-router-dom": "^4.3.1",
    "react-sortable-tree": "^2.6.0",
    "shallowequal": "^1.1.0",
    "shortid": "^2.2.14",
    "slugify": "^1.3.4",
    "url-parse": "^1.4.4"
